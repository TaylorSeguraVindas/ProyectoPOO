                                <Insets bottom="10.0" top="10.0" />
                            </padding>
                        </HBox>
                        <VBox fx:id="contenedorPrincipal" alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0" VBox.vgrow="ALWAYS" />
